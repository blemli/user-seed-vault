{
    "name": "blemli/user-seed-vault",
    "description": "helps you create an encrypted user-seeder",
    "keywords": [
        "Problemli GmbH",
        "laravel",
        "user-seed-vault"
    ],
    "homepage": "https://github.com/blemli/user-seed-vault",
    "support": {
        "issues": "https://github.com/blemli/user-seed-vault/issues",
        "source": "https://github.com/blemli/user-seed-vault"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Graf",
            "email": "grafst@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0",
        "intervention/image": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Blemli\\UserSeedVault\\": "src/",
            "Blemli\\UserSeedVault\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blemli\\UserSeedVault\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blemli\\UserSeedVault\\UserSeedVaultServiceProvider"
            ],
            "aliases": {
                "UserSeedVault": "Blemli\\UserSeedVault\\Facades\\UserSeedVault"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
